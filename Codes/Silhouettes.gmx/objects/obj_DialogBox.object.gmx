        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Press Space

{   
    //if !control then exit;
    if global.option_time then exit;
    if string_length(dialog) != string_length(dialog_full)
        {
        dialog = dialog_full;
        image_alpha = 1;
        exit;
        }
        
    with(obj_DialogCreator) 
        {
        if array_length_2d(dialog,global.dialog_type) &gt; global.count+1
            {
            var str = string_copy(dialog[global.dialog_type,global.count+1],1,3);
            if string_copy(str,1,3) = "Opt"
                {
                if instance_number(obj_DialogReplyBox) = 0
                    {
                    global.dialog_time = true;
                    var opt = string_copy(dialog[global.dialog_type,global.count+1],8,9);
                    for(var i = 0; i &lt; real(opt); i++)
                        {
                        global.option_id[i] = instance_create(view_xview[0]+(view_wview[0]*4)/7,view_yview[0]+view_hview[0]/3+(i*32),obj_DialogReplyBox);     
                        if i = 0 then with(global.option_id[i]) image_index = 1;
                        global.option_id[i].number = i+1;
                        global.option_id[i].option = options[global.dialog_type,i];
                        }
                    exit;
                    }
                }
            }
        
        global.count++;
        if array_length_2d(dialog,global.dialog_type) &gt; global.count &amp;&amp; global.dialog_type != SILHOUETTES then event_user(0); //Sıradaki dialoğa geç.
        else with(other) 
            {
            alarm[2] = 1;
            control = false;
            }
        }
    
}
</string>
