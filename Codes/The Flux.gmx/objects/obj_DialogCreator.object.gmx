    dialog[SILHOUETTE,0] = "I don't believe I'm in the dark. I'm only here to observe those who dwell in darkness.";
    dialog[SILHOUETTE,1] = "Different times, different places... But the act is always the same. Some thinks that only they deserve to live.";
    dialog[SILHOUETTE,2] = "Existence is pain.";
    dialog[SILHOUETTE,3] = "They say there is a secret that lies in the furthest east of the road." ;
    dialog[SILHOUETTE,4] = "Everything is meaningful in this world. Every object says something about this world and those who live in it. I'm exploring their depths to find myself.";
    dialog[SILHOUETTE,5] = "Just consume everything you can find here, death will nullify your search anyway.";
  
    dialog[SILHOUETTE2,0] = "Everyone wants to feel strong. Therefore, some of them choose the easiest way.";
